  "homepage": "https://dcoderdude.github.io/portfolio-website/",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.33",
